.content{
    width: 100%;
    height: 100%;
    padding: 0 10px;
    box-sizing: border-box;
}
.icon{
    width: 30px;
    height: 30px;
    margin: 0 10px;
}
.text{
    width: calc(100% - 50px);
    height: 100%;
    border-bottom: 1px solid #cccccc;
    box-sizing: border-box;
    font-size: 18px;
    display: flex;
    align-items: center;
}
.rank_item{
    height: 40px;
    display: flex;
    align-items: center;
}
.collapse{
    justify-content: flex-end;
}